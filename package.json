{
  "name": "@iofate/kucoin-ws",
  "version": "1.7.2",
  "description": "Node.js websocket client for KuCoin",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src --ext .js,.ts",
