{
  "name": "@iofate/kucoin-ws",
  "version": "1.8.0",
  "description": "Node.js websocket client for KuCoin",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src --ext .js,.ts",
    "typecheck": "tsc --noEmit",
    "test": "npm run lint && npm run typecheck && npm run build",
    "start": "vite-node tests/client.ts",
    "prerelease": "npm run build",
    "release": "standard-version"
  },
  "keywords": [
    "kucoin",
    "websocket",
    "ws",
    "cryptocurrency",
    "exchange",
    "nodejs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IOfate/kucoin-ws.git"
  },
  "bugs": {
    "url": "https://github.com/IOfate/kucoin-ws/issues"
  },
  "author": "IOfate",
  "license": "MIT",
  "dependencies": {
    "emittery": "^1.0.3",
    "got": "^14.2.1",
    "parse-duration": "^1.1.0",
    "queue": "^7.0.0",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^52.0.0",
    "prettier": "^3.2.5",
    "standard-version": "^9.5.0",
    "typescript": "^5.4.5",
    "vite-node": "^1.5.3"
  },
  "files": [
    "dist/**/*.{js,ts,map}",
    "CHANGELOG"
  ],
  "engines": {
    "node": ">=18"
  }
